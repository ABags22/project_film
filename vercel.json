{
  "version": 2,
  "builds": [
    { "src": "backend_api/dist/index.js", "use": "@vercel/node" },
    { "src": "lanepage_film/package.json", "use": "@vercel/static-build" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "backend_api/dist/index.js" },
    { "src": "/(.*)", "dest": "lanepage_film/$1" }
  ]
}
